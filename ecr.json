{
    "Parameters": {
      "repositoryName": {
        "Type": "hello-world"
      }
    },
    "Resources": {
      "MyRepository": {
        "Type": "AWS::ECR::Repository",
        "Properties": {
            "RepositoryName": {
                "Ref": "repositoryName"
            }
        }
      }
    },
    "Outputs": {
      "Arn": {
        "Value": {
          "Fn::GetAtt": [
            "MyRepository",
            "Arn"
          ]
        }
      }
    }
}